{
    "autoscale": {
        "min_workers": 1,
        "max_workers": 4
    },
    "cluster_name": "Dock_General",
    "spark_version": "12.2.x-scala2.12",
    "spark_conf": {
        "spark.databricks.service.server.enabled": "true",
        "spark.databricks.dataLineage.enabled": "true",
        "spark.databricks.hive.metastore.glueCatalog.enabled": "true",
        "spark.driver.memory": "8g",
        "spark.hadoop.fs.s3a.acl.default": "BucketOwnerFullControl",
        "spark.sql.sources.partitionOverwriteMode": "dynamic",
        "spark.databricks.service.port": "15001"
    },
    "aws_attributes": {
        "first_on_demand": 1,
        "availability": "SPOT_WITH_FALLBACK",
        "zone_id": "us-east-1a",
        "instance_profile_arn": "arn:aws:iam::463684499885:instance-profile/DatabricksGlueHML",
        "spot_bid_price_percent": 100,
        "ebs_volume_count": 0
    },
    "node_type_id": "c6gd.8xlarge",
    "driver_node_type_id": "c6gd.8xlarge",
    "ssh_public_keys": [],
    "custom_tags": {},
    "cluster_log_conf": {
        "s3": {
            "destination": "s3://dock-databricks-overwatch/cluster-logs/",
            "region": "us-east-1",
            "enable_encryption": true,
            "canned_acl": "bucket-owner-full-control"
        }
    },
    "spark_env_vars": {
        "AWS_STS_REGIONAL_ENDPOINTS": "\"regional\""
    },
    "autotermination_minutes": 15,
    "enable_elastic_disk": false,
    "init_scripts": [],
    "enable_local_disk_encryption": false,
    "runtime_engine": "STANDARD"

}